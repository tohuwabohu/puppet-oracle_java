{
    "name": "tohuwabohu-java",
    "version": "1.0.0",
    "author": "Martin Meinhold",
    "license": "Apache License, Version 2.0",
    "summary": "Manage the installation of Java packages",
    "source": "https://github.com/tohuwabohu/puppet-java.git",
    "project_page": "https://github.com/tohuwabohu/puppet-java",
    "issues_url": "https://github.com/tohuwabohu/puppet-java/issues",
    "tags": [ "java", "jdk", "jre" ],
    "operatingsystem_support": [
        {
            "operatingsystem": "Debian",
            "operatingsystemrelease": [ "6", "7" ]
        }
    ],
    "requirements": [
        {
            "name": "puppet",
            "version_requirement": "3.x"
        }
    ],
    "dependencies": [
        {
            "name": "puppetlabs/apt",
            "version_requirement": ">= 1.4.2 < 2.0.0"
        },
        {
            "name": "puppetlabs/concat",
            "version_requirement": ">= 1.1.0 < 2.0.0"
        },
        {
            "name": "ripienaar/module_data",
            "version_requirement": "0.x"
        },
        {
            "name": "puppetlabs/stdlib",
            "version_requirement": "4.x"
        }
    ]
}
