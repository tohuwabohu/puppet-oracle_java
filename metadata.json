{
    "name": "tohuwabohu-oracle_java",
    "version": "1.0.0",
    "author": "Martin Meinhold",
    "license": "Apache License, Version 2.0",
    "summary": "Manage the installation of Oracle Java packages for Debian-based systems",
    "source": "https://github.com/tohuwabohu/puppet-oracle_java.git",
    "project_page": "https://github.com/tohuwabohu/puppet-oracle_java",
    "issues_url": "https://github.com/tohuwabohu/puppet-oracle_java/issues",
    "tags": [ "java", "jdk", "jre", "oracle" ],
    "operatingsystem_support": [
        {
            "operatingsystem": "Debian",
            "operatingsystemrelease": [ "7" ]
        }
    ],
    "requirements": [
        {
            "name": "puppet",
            "version_requirement": "3.x"
        }
    ],
    "dependencies": [
        {
            "name": "puppetlabs/apt",
            "version_requirement": "2.x"
        },
        {
            "name": "puppetlabs/concat",
            "version_requirement": ">= 1.1.0 < 3.0.0"
        },
        {
            "name": "puppetlabs/stdlib",
            "version_requirement": "4.x"
        }
    ]
}
